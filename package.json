    "commander": "^5.1.0",
    "conseiljs": "5.0.4-beta.1",
    "loglevel": "^1.6.8",
    "node-fetch": "^2.6.0",
    "@tacoinfra/harbinger-lib": "1.0.27",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tacoinfra/harbinger-cli.git"
  },
  "keywords": [
